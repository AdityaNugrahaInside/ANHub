    Desc = "Daftar musuh update otomatis.",
    Values = GetEnemiesList(),
    Multi = true,
    AllowNone = true,
    Flag = "EnemySelector_Config",
    Callback = function(val)
        Config.SelectedEnemies = {}
        if typeof(val) == "table" then
